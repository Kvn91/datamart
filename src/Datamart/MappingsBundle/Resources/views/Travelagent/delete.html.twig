{% extends "DatamartMappingsBundle::layout.html.twig" %}

{% block title %}
    Supprimer un Travel Agent - {{ parent() }}
{% endblock %}

{% block datamartmappings_body %}

    <h2>Supprimer un Travel Agent</h2>

    <p>
        Etes-vous certain de vouloir supprimer le Travel Agent "{{ travelagent.originalField }}" ?
    </p>

    <form action="{{ path('datamart_mappings_travelagent_delete', {'id': travelagent.id}) }}" method="post">
    <input type="submit" value="Supprimer" class="btn btn-danger" />
    {{ form_end(form) }}
    <br/>
    <p>
        <a href="{{ path('datamart_mappings_travelagent') }}" class="btn btn-default">
            <i class="glyphicon glyphicon-chevron-left"></i>
            Retour Ã  la liste de Travel Agents
        </a>
    </p>
{% endblock %}