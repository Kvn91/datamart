{% extends "DatamartMappingsBundle::layout.html.twig" %}

{% block title %}
    Supprimer un Room Type - {{ parent() }}
{% endblock %}

{% block datamartmappings_body %}

    <h2>Supprimer un Room Type</h2>

    <p>
        Etes-vous certain de vouloir supprimer le Room Type "{{ roomtype.originalField }}" ?
    </p>

    <form action="{{ path('datamart_mappings_roomtype_delete', {'id': roomtype.id}) }}" method="post">
    <input type="submit" value="Supprimer" class="btn btn-danger" />
    {{ form_end(form) }}
    <br/>
    <p>
        <a href="{{ path('datamart_mappings_roomtype') }}" class="btn btn-default">
            <i class="glyphicon glyphicon-chevron-left"></i>
            Retour Ã  la liste de Room Types
        </a>
    </p>
{% endblock %}