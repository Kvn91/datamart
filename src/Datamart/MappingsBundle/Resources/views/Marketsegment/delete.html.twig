{% extends "DatamartMappingsBundle::layout.html.twig" %}

{% block title %}
    Supprimer un Market Segment - {{ parent() }}
{% endblock %}

{% block datamartmappings_body %}

    <h2>Supprimer un Market Segment</h2>

    <p>
        Etes-vous certain de vouloir supprimer le Market Segment "{{ marketsegment.originalField }}" ?
    </p>

    <form action="{{ path('datamart_mappings_marketsegment_delete', {'id': marketsegment.id}) }}" method="post">
    <input type="submit" value="Supprimer" class="btn btn-danger" />
    {{ form_end(form) }}
    <br/>
    <p>
        <a href="{{ path('datamart_mappings_marketsegment') }}" class="btn btn-default">
            <i class="glyphicon glyphicon-chevron-left"></i>
            Retour Ã  la liste de Market Segments
        </a>
    </p>
{% endblock %}