{% extends "DatamartMappingsBundle::layout.html.twig" %}

{% block title %}
    Supprimer un Reservation Status - {{ parent() }}
{% endblock %}

{% block datamartmappings_body %}

    <h2>Supprimer un Reservation Status</h2>

    <p>
        Etes-vous certain de vouloir supprimer le Reservation Status "{{ reservationstatus.originalField }}" ?
    </p>

    <form action="{{ path('datamart_mappings_reservationstatus_delete', {'id': reservationstatus.id}) }}" method="post">
    <input type="submit" value="Supprimer" class="btn btn-danger" />
    {{ form_end(form) }}
    <br/>
    <p>
        <a href="{{ path('datamart_mappings_reservationstatus') }}" class="btn btn-default">
            <i class="glyphicon glyphicon-chevron-left"></i>
            Retour Ã  la liste de Reservation Status
        </a>
    </p>
{% endblock %}